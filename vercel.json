{
  "builds": [
    { "src": "api/**/*.py", "use": "@vercel/python@0.6.3" },
    { "src": "public/**",   "use": "@vercel/static@1.0.0" }
  ],
  "routes": [
    { "src": "/api/(.*)", "dest": "api/$1" },
    { "src": "/",         "dest": "public/HTML_ata.html" },
    { "src": "/(.*)",     "dest": "public/$1" }
  ]
}
